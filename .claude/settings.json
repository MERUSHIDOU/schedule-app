{
  "permissions": {
    "allow": ["Bash", "Read", "WebFetch"],
    "deny": [
      "Bash(rm *)",
      "Bash(*rm -*)",
      "Bash(rmdir *)",
      "Bash(*rmdir *)",
      "Bash(* rm *)",
      "Bash(git rm *)",
      "Bash(*git rm *)",
      "Bash(git commit * master *)",
      "Bash(git push * master *)"
    ],
    "ask": ["Bash(git commit:*)", "Bash(git push:*)"]
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": ["github"]
}
